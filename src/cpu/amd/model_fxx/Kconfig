	bool
	select ARCH_BOOTBLOCK_X86_32
	select ARCH_VERSTAGE_X86_32
	select ARCH_ROMSTAGE_X86_32
	select ARCH_RAMSTAGE_X86_32
	select MMX
	select SSE
	select SSE2
	select TSC_SYNC_LFENCE
	select UDELAY_LAPIC
	select SUPPORT_CPU_UCODE_IN_CBFS
	select ACPI_HUGE_LOWMEM_BACKUP

