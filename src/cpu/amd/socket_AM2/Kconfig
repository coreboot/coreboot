config CPU_AMD_SOCKET_AM2
	bool
	default n
	select K8_REV_F_SUPPORT
	# Opteron K8 1G HT support
	select K8_HT_FREQ_1G_SUPPORT
	select CPU_AMD_MODEL_FXX

config CPU_SOCKET_TYPE
	hex
	default 0x11
	depends on CPU_AMD_SOCKET_AM2

# DDR2 and REG
config DIMM_SUPPORT
	hex
	default 0x0004
	depends on CPU_AMD_SOCKET_AM2

