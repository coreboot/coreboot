config CPU_AMD_SOCKET_AM3
	bool
	select CPU_AMD_MODEL_10XXX
	select HT3_SUPPORT
	select PCI_IO_CFG_EXT
	select CACHE_AS_RAM
	select X86_AMD_FIXED_MTRRS

config CPU_SOCKET_TYPE
	hex
	default 0x11
	depends on CPU_AMD_SOCKET_AM3

config EXT_RT_TBL_SUPPORT
	bool
	default n
	depends on CPU_AMD_SOCKET_AM3

config EXT_CONF_SUPPORT
	bool
	default n
	depends on CPU_AMD_SOCKET_AM3

config CBB
	hex
	default 0x0
	depends on CPU_AMD_SOCKET_AM3

config CDB
	hex
	default 0x18
	depends on CPU_AMD_SOCKET_AM3

config XIP_ROM_SIZE
	hex
	default 0x80000
	depends on CPU_AMD_SOCKET_AM3

