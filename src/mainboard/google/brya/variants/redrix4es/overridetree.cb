				end
			end
			chip drivers/i2c/max98390
				register "desc" = ""MAX98390 Speaker Amp 2""
				register "uid" = "2"
				register "name" = ""MXW2""
				register "r0_calib_key" = ""dsm_calib_r0_2""
				register "temperature_calib_key" = ""dsm_calib_temp_2""
