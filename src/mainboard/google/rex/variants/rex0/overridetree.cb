		option UWB_BITBANG		1
		option UWB_GSPI1		2
	end
	field WIFI 18
		option WIFI_CNVI		0
		option WIFI_PCIE		1
	end
	field TOUCHSCREEN 19 20
		option TOUCHSCREEN_I2C		0
		option TOUCHSCREEN_GSPI		1
		option TOUCHSCREEN_THC		2
