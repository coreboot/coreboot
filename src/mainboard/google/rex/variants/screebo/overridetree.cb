fw_config
	field AUDIO 0 1
		option AUDIO_UNKNOWN		0
		option ALC1019_ALC5682I_I2S	1
	end
	field DB_CONFIG 2 4
		option DB_UNKNOWN		0
		option DB_TYPEC			1
		option DB_TBT			2
	end
	field MB_CONFIG 5 7
		option MB_UNKNOWN		0
		option MB_TYPEC			1
		option MB_TBT			2
	end
	field TOUCHSCREEN 8 9
		option TOUCHSCREEN_UNKNOWN	0
		option TOUCHSCREEN_ILITEK	1
		option TOUCHSCREEN_ELAN		2
	end
	field FP_MCU 10
		option FP_MCU_STM		0
		option FP_MCU_NPCX		1
	end
	field DB_SD 11 12
		option SD_GL9750		0
		option SD_RTS5227S		1
	end
	field WIFI_SAR_ID 13
