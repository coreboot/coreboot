chip cpu/samsung/exynos5250
	device cpu_cluster 0 on end
	register "xres" = "1366"
	register "yres" = "768"
	register "bpp" = "16"
	# complex magic timing!
	register "clkval_f" = "2"
	register "upper_margin" = "14"
	register "lower_margin" = "3"
	register "vsync" = "5"
	register "left_margin" = "80"
	register "right_margin" = "48"
	register "hsync" = "32"
end
